{{!-- Email subscribe form at the Top of the page --}}
{{#if @labs.members}}

<div class="godo-header">
    <div class="container">
        <div class="row">
            <div class="col s12 m8 u-flexCenter u-md-block">
                <h4 class="u-fontWeightSemibold u-relative u-fontSize28 u-paddingRight20">{{t "Newsletter"}}</h4>
                <p>{{t "Stay up to date! Get all the latest & greatest posts delivered straight to your inbox"}}</p>
            </div>
            <div class="col s12 m4 u-textAlignRight">
                <div class="button button--large button--outline-success u-md-block">
                    {{t "Subscribe"}}
                    <svg class="icon icon--md"><use xlink:href="#icon-arrow"></use></svg>
                </div>
            </div>
        </div>
    </div>
</div>

{{/if}}